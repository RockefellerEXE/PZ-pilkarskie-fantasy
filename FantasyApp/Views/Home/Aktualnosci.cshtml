@{
    ViewData["Title"] = "Aktualności";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="~/css/aktualnosci.css" rel="stylesheet" />
</head>
<body>
    <div class="header">
        <h1>Aktualności</h1>
    </div>
    <div class="container">
        <div class="table-section">
            <h2>Zmiany cen zawodników</h2>
            <img src="https://via.placeholder.com/400x200?text=Tabela+zmian+cen" alt="Tabela zmian cen">
            <div class="pagination">
                <button class="active">1</button>
                <button>2</button>
                <button>3</button>
                <button>...</button>
                <button>10</button>
            </div>
        </div>
        <div class="table-section">
            <h2>Najważniejsze tweety</h2>
            <img src="https://via.placeholder.com/400x200?text=Tabela+tweetów" alt="Tabela tweetów">
            <div class="pagination">
                <button class="active">1</button>
                <button>2</button>
                <button>3</button>
                <button>...</button>
                <button>10</button>
            </div>
        </div>
    </div>
</body>
</html>